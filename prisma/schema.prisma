datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id        String @id @default(cuid())
  firstName String
  lastName  String
  email     String
  avatar    String @default("")
}

model User {
  id     String @id @default(cuid()) 
  userName String
  email String
  password String
  createdAt DateTime @default(now())
}
